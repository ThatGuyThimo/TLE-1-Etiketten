{
  "expo": {
    "name": "labelvision",
    "slug": "labelvision",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Mee_rotterdam_rijnmond.png",
      "resizeMode": "cover",
      "backgroundColor": "#F06B05"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "labelvision needs access to your Camera."
      },
      "bundleIdentifier": "com.labelvision.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets//Mee_rotterdam_rijnmond.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.labelvision.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "enableCodeScanner": true,
          "cameraPermissionText": "labelvision needs access to your Camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "09cc2342-b2d9-489a-95fe-466dfc9e417b"
      }
    }
  }
}
